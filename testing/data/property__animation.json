{
  "name": "animation",
  "type": "property",
  "syntax": "<single-animation>#",
  "constituents": [
    {
      "type": "single-animation",
      "syntax": "<time [0s,∞]> || <easing-function> || <time> || <single-animation-iteration-count> || <single-animation-direction> || <single-animation-fill-mode> || <single-animation-play-state> || [ none | <keyframes-name> ]"
    },
    {
      "type": "time",
      "syntax": "<s> | <ms>"
    },
    {
      "type": "easing-function",
      "syntax": "linear | <linear-easing-function> | <cubic-bezier-easing-function> | <step-easing-function>"
    },
    {
      "type": "single-animation-iteration-count",
      "syntax": "infinite | <number [0,∞]>"
    },
    {
      "type": "single-animation-direction",
      "syntax": "normal | reverse | alternate | alternate-reverse"
    },
    {
      "type": "single-animation-fill-mode",
      "syntax": "none | forwards | backwards | both"
    },
    {
      "type": "single-animation-play-state",
      "syntax": "running | paused"
    },
    {
      "type": "keyframes-name",
      "syntax": "<custom-ident> | <string>"
    },
    {
      "type": "linear-easing-function",
      "syntax": "linear(<linear-stop-list>)"
    },
    {
      "type": "cubic-bezier-easing-function",
      "syntax": "ease | ease-in | ease-out | ease-in-out | cubic-bezier(<number [0,1]>, <number>, <number [0,1]>, <number>)"
    },
    {
      "type": "step-easing-function",
      "syntax": "step-start | step-end | steps(<integer>[, <step-position>]?)"
    },
    {
      "type": "linear-stop-list",
      "syntax": "[ <linear-stop> ]#"
    },
    {
      "type": "step-position",
      "syntax": "jump-start | jump-end | jump-none | jump-both | start | end"
    },
    {
      "type": "linear-stop",
      "syntax": "<number> && <linear-stop-length>?"
    },
    {
      "type": "linear-stop-length",
      "syntax": "<percentage>{1,2}"
    }
  ]
}