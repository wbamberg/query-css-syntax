{
  "name": "@counter-style/symbols",
  "type": "at-rule-descriptor",
  "syntax": "<symbol>+",
  "constituents": [
    {
      "type": "symbol",
      "syntax": "<string> | <image> | <custom-ident>"
    },
    {
      "type": "image",
      "syntax": "<url> | <gradient>"
    },
    {
      "type": "url",
      "syntax": "<url()> | <src()>"
    },
    {
      "type": "url()",
      "syntax": "url( <string> <url-modifier>* ) | <url-token>"
    },
    {
      "type": "src()",
      "syntax": "src( <string> <url-modifier>* )"
    }
  ]
}