{
  "name": "@import",
  "type": "at-rule",
  "syntax": "@import [ <url> | <string> ] [ layer | layer(<layer-name>) ]? <import-conditions> ;",
  "constituents": [
    {
      "type": "url",
      "syntax": "<url()> | <src()>"
    },
    {
      "type": "layer-name",
      "syntax": "<ident> [ '.' <ident> ]*"
    },
    {
      "type": "import-conditions",
      "syntax": "[ supports( [ <supports-condition> | <declaration> ] ) ]? <media-query-list>?"
    },
    {
      "type": "url()",
      "syntax": "url( <string> <url-modifier>* ) | <url-token>"
    },
    {
      "type": "src()",
      "syntax": "src( <string> <url-modifier>* )"
    },
    {
      "type": "supports-condition",
      "syntax": "not <supports-in-parens> | <supports-in-parens> [ and <supports-in-parens> ]* | <supports-in-parens> [ or <supports-in-parens> ]*"
    },
    {
      "type": "supports-in-parens",
      "syntax": "( <supports-condition> ) | <supports-feature> | <general-enclosed>"
    },
    {
      "type": "supports-feature",
      "syntax": "<supports-decl>"
    },
    {
      "type": "general-enclosed",
      "syntax": "[ <function-token> <any-value>? ) ] | [ ( <any-value>? ) ]"
    },
    {
      "type": "supports-decl",
      "syntax": "( <declaration> )"
    }
  ]
}